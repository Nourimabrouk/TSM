# GetOptKalman
state_space_parameter_optimizer <- function(df_data, phi_ini){
print(phi_ini)
results <- optim(par=phi_ini, fn=function(par) - GetloglikGauss(df_data, par), method='BFGS')
theta_hat <- results$par
print("The parameter estimates are:")
print(round(theta_hat, 4))
print("The log-likelihood value is:")
print(results$value)
print("iterations:")
print(results$counts[1])
cat("Exit flag:")
print(results$convergence) # zero indicates succesfull optimization
return(theta_hat)
}
#GetloglikGauss
GetloglikGauss<- function(data, theta){
y <- as.matrix(data)
kf_state <- KalmanFilterSV(data, theta)
h <- kf_state$h
P <- kf_state$P
loglik_density <- -(1/2)*log(2*pi) - (1/2)*log(P) - (1/2)*((y-h)^2)/(2*P)
loglik_density[is.nan(loglik_density)] <- 0
loglikelihood <- (sum(loglik_density))
return(loglikelihood)
}
# KalmanFilterSV
KalmanFilterSV <- function(data, theta){
y <- as.matrix(data)
n <- length(y)
sig_u <- pi^2/2
mean_u <- -1.27
sig_sq_eta <- theta[1]
phi <- theta[2]
omega <- theta[3]
# Kalman filtering
h <- rep(0,n)
P <- rep(0,n)
v <- rep(0,n)
F <- rep(0,n)
K <- rep(0,n)
h[1] <- omega/(1-phi) #unconditional mean
P[1] <- sig_sq_eta/(1-phi^2) #unconditional variance
for (t in 2:n) {
v[t] <- y[t] - h[t] - mean_u
F[t] <- P[t] + sig_u
K[t] <- phi*(P[i]/F[t])
if(i < n-1){
h[t+1] <- omega + phi*h[t] + K[t]*v[t]
P[t+1] <- (phi^2)*P[t] + sig_sq_eta^4 - (K[t]^2)*F[t]
}
}
kalmanfiltersv <- data.frame(h, P, v, F, K)
return(kalmanfiltersv)
}
