# Smoothed State
a <- kalman$a
P <- kalman$P
v <- kalman$v
F <- kalman$F
K <- kalman$K
SmoothedState_df <- SmoothedState(a,P,v,F,K)
# Disturbances Smoothing
F <- kalman$F
V <- kalman$v
k <- kalman$K
r <- SmoothedState_df$r
N <- SmoothedState_df$N
disturbance <- DisturbancesSmoothing(F,V,K,r,N)
# Results
disturbance
SmoothedState_df
kalman
